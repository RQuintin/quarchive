{
    "manifest_version": 2,
    "name": "Quartermarker",
    "version": "0.1",
    "permissions": [
        "bookmarks",
        "storage",
        "unlimitedStorage"
    ],
    "background": {
        "scripts": ["quartermarker.js"]
    },
    "options_ui": {
        "page": "options.html",
        "browser_style": true,
        "chrome_style": true
    }
}
